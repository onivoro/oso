{
    "name": "@onivoro/oso",
    "version": "22.0.0",
    "repository": {
        "url": "git+https://github.com/onivoro/oso.git"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/*"
    ],
    "scripts": {
        "onx": "onx",
        "bind": "onx Build && echo && chmod +x dist/esm/index.js && npm link",
        "build": "onx Build",
        "deploy": "onx Publish",
        "test": "onx Test",
        "update": "onx Update"
    },
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js",
            "default": "./dist/esm/lib.js"
        }
    },
    "onx": {
        "platform": "server",
        "module": "commonjs"
    },
    "devDependencies": {
        "@onivoro/cli": "^22.0.3",
        "@types/jest": "*",
        "@types/node": "22.7.9",
        "typescript": "*"
    },
    "engines": {
        "node": "22.10.0",
        "npm": "10.9.0"
    },
    "dependencies": {
        "@nestjs/common": "^10.4.6",
        "@onivoro/server-aws-s3": "^22.0.0",
        "@onivoro/server-build": "^22.0.0",
        "@onivoro/server-common": "^22.0.0",
        "@onivoro/server-disk": "^22.0.1",
        "@onivoro/server-parameterization": "^22.0.0",
        "@onivoro/server-process": "^22.0.0",
        "nest-commander": "^3.15.0"
    },
    "bin": {
        "oso": "dist/esm/index.js"
    }
}
